# 生产环境配置

# API基础URL  
API_BASE_URL=https://keepthinking.me


# 环境类型
ENV_TYPE=prod

# 是否开启调试模式
DEBUG=false

# 是否开启日志
ENABLE_LOGGING=false

# 网络超时时间(毫秒)
CONNECT_TIMEOUT=15000
RECEIVE_TIMEOUT=15000
SEND_TIMEOUT=15000

# 是否开启网络缓存
ENABLE_CACHE=true

# 缓存过期时间(分钟)
CACHE_EXPIRE_MINUTES=10

# 最大重试次数
MAX_RETRY_COUNT=2

# 应用名称
APP_NAME=扎根理论垂直智能体

# 默认分页大小
DEFAULT_PAGE_SIZE=20
